{
  "name": "template-react-vite",
  "description": "A modern React template with Vite, TypeScript, Ant Design, and UnoCSS",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "license": "MIT",
  "keywords": [
    "react",
    "vite",
    "typescript",
    "antd",
    "unocss",
    "template"
  ],
  "author": "PureSilent",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mobaibai/template-react-vite.git"
  },
  "bugs": {
    "url": "https://github.com/mobaibai/template-react-vite/issues"
  },
  "homepage": "https://github.com/mobaibai/template-react-vite#readme",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "deploy": "pnpm run build && gh-pages -d dist",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\""
  },
  "dependencies": {
    "@ant-design/v5-patch-for-react-19": "^1.0.3",
    "@react-spring/web": "^10.0.1",
    "antd": "^5.26.6",
    "axios": "^1.11.0",
    "classnames": "^2.5.1",
    "crypto-js": "^4.2.0",
    "nanoid": "^5.1.5",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.7.0",
    "swr": "^2.3.4",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.31.0",
    "@iconify/json": "^2.2.362",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/crypto-js": "^4.2.2",
    "@types/node": "^24.1.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react-swc": "^3.11.0",
    "eslint": "^9.31.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "gh-pages": "^6.3.0",
    "globals": "^16.3.0",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.2.0",
    "sass": "^1.89.2",
    "svgo": "^4.0.0",
    "svgstore": "^3.0.1",
    "terser": "^5.43.1",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.38.0",
    "unocss": "^66.3.3",
    "vite": "^7.0.5"
  }
